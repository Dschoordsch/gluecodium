package namerules

interface NameRules {
    public typedef NameRules is undefined

    array StringArray of String

    struct ExampleStruct {
        Double doubleValue
        Int64[] intValues
    }

    enumeration ExampleError {
        None,
        Fatal
    }

    attribute UInt32 intAttribute
    attribute Boolean booleanAttribute
    attribute ExampleStruct structAttribute

    method create {}

    method someMethod {
        in {
            ExampleStruct someArgument
        }
        out {
            Double value
        }
        error ExampleError
    }
}
