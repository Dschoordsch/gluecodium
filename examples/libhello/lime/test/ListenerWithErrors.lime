package test

import another.AdditionalErrors.ExternalErrorsException

interface ListenerWithErrors {
    fun getMessage(): String throws ExternalErrorsException
    fun setMessage(
        message: String
    ) throws ExternalErrorsException
}

class ErrorMessenger {
    fun getMessage(
        listener: ListenerWithErrors
    ): String throws ExternalErrorsException
    fun setMessage(
        listener: ListenerWithErrors,

        message: String
    ) throws ExternalErrorsException
    constructor create()
}
