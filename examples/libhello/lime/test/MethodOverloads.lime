package test

class MethodOverloads {
    struct Point {
        x: Double = 0.0
        y: Double = 0.0
    }
    typealias StringArray = List<String>
    typealias IntArray = List<Byte>
    typealias StringSet = Set<String>
    typealias IntSet = Set<Byte>
    static fun isBoolean(
        input: Boolean
    ): Boolean
    static fun isBoolean(
        input: Byte
    ): Boolean
    static fun isBoolean(
        input: String
    ): Boolean
    static fun isBoolean(
        input: Point
    ): Boolean
    static fun isBoolean(
        input1: Boolean,
        input2: Byte,
        input3: String,
        input4: Point
    ): Boolean
    @Java("isBooleanStringArrayOverload")
    static fun isBoolean(
        input: StringArray
    ): Boolean
    @Java("isBooleanIntArrayOverload")
    static fun isBoolean(
        input: IntArray
    ): Boolean
    @Java("isBooleanStringSetOverload")
    static fun isBoolean(
        input: StringSet
    ): Boolean
    @Java("isBooleanIntSetOverload")
    static fun isBoolean(
        input: IntSet
    ): Boolean
}

interface SpecialNames {
    fun create()
    fun release()
    fun createProxy()
}

class ConstructorOverloads {
    enum ErrorEnum {
        NONE,
        CRASHED
    }
    constructor create()
    constructor create(
        input: String
    )
    constructor create(
        input: Boolean
    )
    constructor create(
        stringInput: String,
        booleanInput: Boolean
    )
    constructor create(
        input: Double
    ) throws ErrorEnumException
    constructor create(
        input: List<Double>
    )
    exception ErrorEnumException(ErrorEnum)
}

class ChildConstructorOverloads : ConstructorOverloads {
    @Java("createNoParametersChild")
    constructor create()
    @Java("createWithErrorChild")
    constructor create(
        input: Double
    ) throws ConstructorOverloads.ErrorEnumException
}
