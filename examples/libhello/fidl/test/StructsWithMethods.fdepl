        Equatable = true
        Companion = "test.StructsWithMethods_Vector_Companion"
    }
}

define GeniumExtensions for interface test.StructsWithMethodsInterface
{
    struct Vector3 {
        Equatable = true
        Companion = "test.StructsWithMethodsInterface_Vector3_Companion"
    }
}

