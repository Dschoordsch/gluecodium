package smoke

class Attributes {
    enum InternalError {
        ERROR_NONE,
        ERROR_FATAL = 999
    }

    struct ExampleStruct {
        value: Double
    }

    property builtInTypeAttribute: UInt {
        get
        set
    }
    property readonlyAttribute: Float {
        get
    }
    property structAttribute: ExampleStruct {
        get
        set
    }
    property arrayAttribute: List<String> {
        get
        set
    }
    property complexTypeAttribute: InternalError {
        get
        set
    }
    property byteBufferAttribute: Blob {
        get
        set
    }
    property instanceAttribute: AttributesInterface {
        get
        set
    }
    property booleanAttribute: Boolean {
        get
        set
    }
    static property staticAttribute: String {
        get
        set
    }
    static property staticReadonlyAttribute: ExampleStruct {
        get
    }
}
