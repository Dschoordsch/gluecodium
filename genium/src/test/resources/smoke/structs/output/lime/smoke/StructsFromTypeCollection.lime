package smoke

import smoke.TypeCollection.Point
import smoke.TypeCollection.Line
import smoke.TypeCollection.Color
import smoke.TypeCollection.ColoredLine
import smoke.TypeCollection.AllTypesStruct

class StructsFromTypeCollection {
    static fun createPoint(
        x: Double,
        y: Double
    ): Point
    static fun swapPointCoordinates(
        input: Point
    ): Point
    static fun createLine(
        pointA: Point,
        pointB: Point
    ): Line
    static fun createColoredLine(
        line: Line,
        color: Color
    ): ColoredLine
    static fun modifyAllTypesStruct(
        input: AllTypesStruct
    ): AllTypesStruct
}
