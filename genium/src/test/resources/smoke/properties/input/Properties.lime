package smoke

class Properties {
    struct ExampleStruct {
        value: Double
    }

    enum InternalError {
        ERROR_NONE,
        ERROR_FATAL = 999
    }

    property builtInTypeAttribute: UInt { get set }
    property readonlyAttribute: Float { get }
    property structAttribute: ExampleStruct
    property arrayAttribute: List<String>
    property complexTypeAttribute: InternalError
    property byteBufferAttribute: Blob
    property instanceAttribute: PropertiesInterface
    property booleanAttribute: Boolean
    static property staticAttribute: String { get set }
    static property staticReadonlyAttribute: ExampleStruct { get }
}

interface PropertiesInterface {
    struct ExampleStruct {
        value: Double
    }

    property structAttribute: ExampleStruct { get set }
}
