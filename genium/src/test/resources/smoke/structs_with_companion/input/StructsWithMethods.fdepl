import "classpath:/GeniumExtensions.fdepl"
import "./StructsWithMethods.fidl"

define GeniumExtensions for typeCollection smoke.StructsWithMethods
{
    struct Vector {
        Companion = "smoke.StructsWithMethods_Vector_Companion"
    }
}

define GeniumExtensions for interface smoke.StructsWithMethodsInterface
{
    struct Vector3 {
        Companion = "smoke.StructsWithMethodsInterface_Vector3_Companion"
    }
}

define GeniumExtensions for interface smoke.StructsWithMethods_Vector_Companion
{
    method validate {
        Static = true
    }

    method create:xy {
        Constructor = true
    }

    method create:copy {
        Constructor = true
    }
}

define GeniumExtensions for interface smoke.StructsWithMethodsInterface_Vector3_Companion
{
    method validate {
        Static = true
    }

    method create:string {
        Constructor = true
    }

    method create:copy {
        Constructor = true
    }
}
