package namerules

class NameRules {
    struct ExampleStruct {
        value: Double
        intValue: List<Long>
    }

    enum ExampleError {
        None,
        Fatal
    }

    typealias StringArray = List<String>

    constructor create()

    fun someMethod(someArgument: ExampleStruct): Double throws ExampleErrorException

    property intAttribute: UInt { get set }
    property booleanAttribute: Boolean { get set }
    property structAttribute: ExampleStruct { get set }

    exception ExampleErrorException(ExampleError)
}
