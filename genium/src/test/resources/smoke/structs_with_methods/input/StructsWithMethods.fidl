package smoke

typeCollection StructsWithMethods {

    struct Vector {
        Double x
        Double y
    }
}

interface StructsWithMethodsInterface {

    public struct Vector3 {
        Double x
        Double y
        Double z
    }
}

interface StructsWithMethods_Vector_Companion {

    method distanceTo {
        in {
            StructsWithMethods.Vector other
        }
        out {
            Double output
        }
    }

    method add {
        in {
            StructsWithMethods.Vector other
        }
        out {
            StructsWithMethods.Vector output
        }
    }

    method validate {
        in {
            Double x
            Double y
        }
        out {
            Boolean result
        }
    }
}

interface StructsWithMethodsInterface_Vector3_Companion {

    method distanceTo {
        in {
            StructsWithMethodsInterface.Vector3 other
        }
        out {
            Double output
        }
    }

    method add {
        in {
            StructsWithMethodsInterface.Vector3 other
        }
        out {
            StructsWithMethodsInterface.Vector3 output
        }
    }

    method validate {
        in {
            Double x
            Double y
            Double z
        }
        out {
            Boolean result
        }
    }
}
