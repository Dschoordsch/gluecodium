package smoke

class BasicTypes {
    static fun stringFunction(input: String): String
    static fun boolFunction(input: Boolean): Boolean
    static fun floatFunction(input: Float): Float
    static fun doubleFunction(input: Double): Double
    static fun byteFunction(input: Byte): Byte
    static fun shortFunction(input: Short): Short
    static fun intFunction(input: Int): Int
    static fun longFunction(input: Long): Long
    static fun ubyteFunction(input: UByte): UByte
    static fun ushortFunction(input: UShort): UShort
    static fun uintFunction(input: UInt): UInt
    static fun ulongFunction(input: ULong): ULong
}
